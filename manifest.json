{
  "manifest_version": 2,
  "name": "Tolerable Discord",
  "version": "0.1.0",
  "description": "Rolls back some of the Discord enshittification.",
  "icons": {
    "48": "icons/tolerable-48.png"
  },
  "content_scripts": [
    {
      "matches": [
        "*://*.discord.com/*"
      ],
      "js": [
        "tolerable.js"
      ]
    }
  ]
}
